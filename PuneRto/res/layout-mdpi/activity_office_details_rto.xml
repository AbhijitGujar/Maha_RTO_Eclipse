<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="815dp" >

        <TextView
            android:id="@+id/tXT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textView1"
            android:layout_below="@+id/textView1"
            android:layout_marginTop="42dp"
            android:text="Address :"
            android:textColor="#000000"
            android:textSize="21dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_OfficeNameDetails"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_OfficeIdDetails"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="32dp"
            android:text="Office Name"
            android:textColor="#000000"
            android:textSize="29dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_OfficerName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView1"
            android:layout_toRightOf="@+id/txt_OfficeIdDetails"
            android:text="OfficerName"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_Address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView2"
            android:layout_alignLeft="@+id/txt_OfficerName"
            android:text="Address"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/tXT"
            android:layout_below="@+id/tXT"
            android:layout_marginTop="94dp"
            android:text="Tel No :"
            android:textColor="#000000"
            android:textSize="21dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_TelNo1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView2"
            android:layout_alignLeft="@+id/txt_Address"
            android:text="TextView"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_TelNo2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txt_TelNo1"
            android:layout_below="@+id/txt_TelNo1"
            android:layout_marginTop="30dp"
            android:text="TextView"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_telNo3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txt_TelNo2"
            android:layout_below="@+id/txt_TelNo2"
            android:layout_marginTop="38dp"
            android:text="TextView"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_TelNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/txt_telNo3"
            android:layout_below="@+id/txt_telNo3"
            android:layout_marginTop="39dp"
            android:text="TextView"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/txt_EmailAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignLeft="@+id/txt_TelNo"
            android:autoLink="web|email"
            android:text="email Address"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/txt_OfficeNameDetails"
            android:layout_below="@+id/txt_OfficeNameDetails"
            android:layout_marginRight="110dp"
            android:layout_marginTop="59dp"
            android:text="Officer Name:"
            android:textColor="#000000"
            android:textSize="21dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/textView2"
            android:layout_below="@+id/txt_TelNo"
            android:layout_marginTop="56dp"
            android:text="Email:"
            android:textColor="#000000"
            android:textSize="21dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_OfficeIdDetails"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="70dp"
            android:layout_toRightOf="@+id/tXT"
            android:text="MH"
            android:textColor="#000000"
            android:textSize="29dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_Fax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_EmailAddress"
            android:layout_marginTop="27dp"
            android:layout_toRightOf="@+id/txt_OfficeIdDetails"
            android:text="TextView"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/txt_Fax"
            android:layout_toLeftOf="@+id/txt_OfficeIdDetails"
            android:text="Fax:"
            android:textColor="#000000"
            android:textSize="21dp"
            android:textStyle="bold" />
        
    </RelativeLayout>

</ScrollView>