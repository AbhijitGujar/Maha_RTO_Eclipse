	public boolean  checkSpecialCharacters(String strcheck )
	{
		
		Pattern regex = Pattern.compile("[,″&<>′]");
		Matcher matcher = regex.matcher(strcheck);
		if (matcher.find())
			return true;
		else
			return false;
		
		
	}

